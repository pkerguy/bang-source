<config>
    <input>
		<jar in="build/client/desktop-1.1-SNAPSHOT.jar" out="final/desktop.jar"/>
        <jar in="build/client/shared-1.1-SNAPSHOT.jar" out="final/shared.jar"/>

    </input>
	<classpath>
		<jar name="obj/*.jar"/>
	</classpath>
    <keep-names>
        <class template="class * extends com.threerings.presents.dobj.DObject"/>
        <class template="class * instanceof java.io.Serializable"/>
        <class template="class * implements com.threerings.io.Streamable"/>
        <class template="class * extends java.lang.Enum"/>
        <class template="class * extends com.samskivert.swing.Controller"/>
        <class template="class * extends com.threerings.jme.model.ModelController"/>
        <class template="class com.threerings.bang.client.Config"/>
        <class template="class com.threerings.bang.client.BangClient"/>
        <class template="class com.threerings.bang.editor.EditorManager"/>
        <class template="public interface com.threerings.stats.data.StatSet$Container"/>
        <class template="public interface com.threerings.media.util.AStarPathUtil$ExtendedTraversalPred"/>
        <class template="class com.threerings.bang.data.Badge">
            <field template="*"/>
        </class>
        <class template="class com.threerings.bang.data.Stat">
            <field template="*"/>
        </class>
        <class template="class com.threerings.bang.data.Badge">
            <field template="*"/>
        </class>
        <class template="class com.threerings.media.tile.SwissArmyTileSet"/>
        <class template="class com.threerings.media.tile.TrimmedTileSet"/>
        <class access="protected+">
            <field access="protected+"/>
            <method access="protected+"/>
        </class>
    </keep-names>

    <property name="log-file" value="log.xml"/>
</config>
